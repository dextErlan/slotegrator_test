{
    "name": "dexterlan/test-slotegrator",
    "type": "project",
    "description": "Test task",
    "require": {
        "php": "^7.4",
        "doctrine/doctrine-orm-module": "^5.2",
        "laminas/laminas-diactoros": "^2.17",
        "symfony/cache": "^5.4",
        "symfony/console": "^5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    }
}
