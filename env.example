DOCUMENT_ROOT=./www
WEB_LOG_DIR=./docker/logs/nginx
NGINX_CONFIG=./docker/config/nginx

# If you already has the port 80 in use, you can change it (for example if you have Apache)
HOST_MACHINE_UNSECURE_HOST_PORT=8080
HOST_MACHINE_SECURE_HOST_PORT=8043

PHP_INI=./docker/config/php/php.ini
PHP_XDEBUG_INI=./docker/config/php/xdebug_linux_docker.ini

# If you already has the port 3306 in use, you can change it (for example if you have MySQL)
HOST_MACHINE_MYSQL_PORT=3306
HOST_MACHINE_MYSQL_HOST=mysql

MYSQL_DATA_DIR=./docker/data/mysql
MYSQL_LOG_DIR=./docker/logs/mysql

# MySQL root user password
MYSQL_ROOT_PASSWORD=secret

# Database settings: Username, password and database name
MYSQL_USER=admin
MYSQL_PASSWORD=secret
MYSQL_DATABASE=test_db
